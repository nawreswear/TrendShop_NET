    <PackageReference Include="Microsoft.VisualStudio.Web.CodeGeneration.Design" Version="8.0.7" />
    <PackageReference Include="Mollie" Version="0.9.1" />
    <PackageReference Include="Mollie.Api" Version="4.14.0" />
